page {
  background: #f5f5f5;
}

.search-box {
  width: 690rpx;
  height: 70rpx;
  margin: 30rpx auto 20rpx;
  border-radius: 10rpx;
  background: #fff url("../../../../img/170204016.png") no-repeat 20rpx center;
  background-size: 25rpx 25rpx;
}

.search-inp {
  margin-left: 60rpx;
  width: 508rpx;
  height: 70rpx;
  font-size: 28rpx;
  float: left;
  color: #333;
}

.search-btn {
  float: right;
  width: 120rpx;
  height: 70rpx;
  border-radius: 0 10rpx 10rpx 0;
  background: #ff5000;
  color: #fff;
  text-align: center;
  line-height: 70rpx;
  font-size: 28rpx;
}

.address-item {
  width: 630rpx;
  padding: 0 30rpx;
  margin: 0 auto 30rpx;
  border-radius: 10rpx;
  background: #fff;  
  min-height: 1000rpx;
}

.info {
  padding-left: 62rpx;
  height: 164rpx;
  border-bottom: 1rpx solid #ecf0f6;
  position: relative;
  background: url("../../../../img/170204017.png") no-repeat left center;
  background-size: 31rpx 36rpx;
}

.name {
  padding: 46rpx 0 20rpx 0;
  display: flex;
  font-size: 30rpx;
  line-height: 30rpx;
  color: #333;
}

.area {
  display: flex;
  font-size: 26rpx;
  line-height: 26rpx;
  color: #666;
}

.error{
    padding-top: 100rpx;
    text-align: center;
}

.error image{
    width: 100rpx;
    height: 100rpx;
    margin-bottom: 15rpx;    
}

.error .tips{    
    font-size: 28rpx;
    line-height: 34rpx;
    color: #999;
}